   "source": [
    "def bubbleSort(nums):\n",
    "    k=len(nums)\n",
    "    for i in range(0,k):\n",
    "        for j in range(0,k-1):\n",
    "            if nums[j]>nums[j+1]:\n",
    "                nums[j],nums[j+1]=nums[j+1],nums[j]\n",
    "        k=k-1\n",
    "    return nums"
   ]
  },
