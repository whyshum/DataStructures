   "source": [
    "def selectionSort(nums):\n",
    "    for i in range(0,len(nums)):\n",
    "        for j in range(i+1,len(nums)):\n",
    "            if nums[j]<nums[i]:\n",
    "                nums[i],nums[j]=nums[j],nums[i]\n",
    "        \n",
    "    return nums"
   ]
